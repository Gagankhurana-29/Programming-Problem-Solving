#include <bits/stdc++.h>
#include<string.h>
using namespace std;
int main(){
    int i,h;
    string s;
    cin>>s;
    if(s[8]=='P'){
        if(s[0]=='1'&&s[1]=='2'){
        }
        else{
            s[0]+=1;
            s[1]+=2;
        }
    }
    else if(s[8]=='A'){
        if(s[0]=='1'&&s[1]=='2'){
            s[0]='0';
            s[1]='0';
        }
    }
    s.erase(8);
    cout<<s;
    return 0;

}

