#include <bits/stdc++.h>

using namespace std;

int main()
{
    int i,a[3],b[3],c[2];
    c[0]=0;
    c[1]=0;
    for(i=0;i<3;i++)
    cin>>a[i];
    for(i=0;i<3;i++)
    cin>>b[i];
    for(i=0;i<3;i++){
        if(a[i]>b[i])
        c[0]+=1;
        else if(a[i]<b[i])
        c[1]+=1;
    }
    cout<<c[0]<<" "<<c[1];

}
