#include <bits/stdc++.h>

using namespace std;

int main(){
    int i,n;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    cin>>a[i];
    for(i=0;i<n;i++){
        if(a[i]%5>=0&&a[i]%5<=2&&a[i]>38)
        cout<<a[i];
        else if(a[i]%5==3&&a[i]>=38)
        cout<<a[i]+2;
        else if(a[i]%5==4&&a[i]>=38)
        cout<<a[i]+1;
        else
        cout<<a[i];
        if(i>=0&&i<n-1)
        cout<<endl;
    }
    return 0;
}
