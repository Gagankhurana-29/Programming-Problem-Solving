#include<iostream>
using namespace std;
int main()
{
    int k=0,g=0,i,a[100],j,m,n,o,p;
    for(i=0;i<4;i++)
    {
        cin>>a[i];
    }
    m=a[0];
    n=a[1];
    o=a[2];
    p=a[3];
    for(j=1;j<=10000;j++)
    {
        k=m+(n*j);
        g=o+(p*j);
    if(k==g)
    {
        cout<<"YES";
        break;
    }
    }
    if(k!=g)
    {
    cout<<"NO";
    }
    return 0;
}
